{
  "name": "@ryoppippi/str-fns",
  "version": "0.0.10",
  "author": "@ryoppippi",
  "repository": "ryoppippi/str-fns",
  "license": "MIT",
  "exports": "./src/index.ts",
  "importMap": "./import_map.json",
  "tasks": {
    "dev": "deno run --watch ./src/index.ts",
    "check": "deno check ./src/*",
    "test": "deno test --parallel --doc",
    "test:coverage": "deno task test --coverage=coverage src/*",
    "coverage:show": "deno coverage ./coverage",
    "coverage:lco": "deno coverage --lcov ./coverage > ./coverage/lcov.info"
  },
  "fmt": {
    "exclude": [
      "*.md"
    ]
  },
  "keywords": [
    "string",
    "functions",
    "utils",
    "typescript",
    "typesafe"
  ]
}
