{
	"name": "str-fns",
	"description": "a collection of string functions",
	"version": "0.0.4",
	"author": "@ryoppippi",
	"repository": "ryoppippi/str-fns",
	"type": "module",
	"main": "src/index.js",
	"types": "./types/index.d.ts",
	"scripts": {
		"build": "dts-buddy",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test": "vitest",
		"prepare": "husky install",
		"prepublishOnly": "pnpm run lint && publint && vitest --run && pnpm run build"
	},
	"files": [
		"src",
		"types"
	],
	"keywords": [
		"string",
		"functions",
		"utils",
		"typescript",
		"typesafe"
	],
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"dependencies": {
		"type-fest": "^3.12.0"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.60.1",
		"dts-buddy": "^0.1.7",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-jsdoc": "^46.4.3",
		"husky": "^8.0.0",
		"lint-staged": "^13.2.3",
		"prettier": "^2.8.8",
		"publint": "^0.1.15",
		"typescript": "^5.1.6",
		"vite": "^4.3.9",
		"vitest": "^0.32.3"
	},
	"license": "MIT",
	"volta": {
		"node": "18.16.0",
		"pnpm": "8.6.0"
	},
	"engines": {
		"npm": "use pnpm please!",
		"yarn": "use pnpm please!",
		"pnpm": ">=8.6.0"
	}
}
